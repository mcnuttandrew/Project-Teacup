<h1> Sign In </h1>
<form action="<%= session_url%>" method="post">
	  <label>
	    Username
	    <input type="text" name="user[username]" value="">
	  </label>

	  <label>
	    Password
	    <input type="password" name="user[password]" value="">
	  </label>

	  <input type="submit" value="Sign In">
</form>

<%=button_to "Sign Up", new_user_url, method: :get%>